<h1>Questions for Unit <%= params[:unit_id] %></h1>
<div class="row">
	<div class="col-md-8 col-md-offset-2">
		<ul>
			<% @questions.each do |q| %>
			<li>
				<h2><%= q.prompt %></h2>
				<ul>
					<li><%= q.answer1 %></li>
					<li><%= q.answer2 %></li>
					<li><%= q.answer3 %></li>
					<li><%= q.answer4 %></li>
				</ul>
			</li>
			<% end %>
		</ul>
	</div>
</div>